package evchargingstore.facade;

import org.springframework.beans.factory.annotation.Autowired;

import evchargingstore.entity.ChargingSessionInfo;
import evchargingstore.entity.ChargingSessionResponse;
import evchargingstore.service.ChargingSessionService;

public class ChargingSessionServiceFacade {

	@Autowired
	private ChargingSessionService chargingSessionService;

	public ChargingSessionResponse submitChargingSession(ChargingSessionInfo sessionInfo) {
		return chargingSessionService.submitChargingSession(sessionInfo);
	}

	public ChargingSessionResponse suspendChargingSession(String id, ChargingSessionInfo sessionInfo) {
		return chargingSessionService.suspendChargingSession(id, sessionInfo);
	}

}
